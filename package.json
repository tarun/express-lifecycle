  "description": "Support Before, After phases in express request processing.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tarun/express-lifecycle.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "express",
    "filter",
